		"eslint-plugin-svelte": "^3.8.1",
		"globals": "^16.1.0",
		"jsdom": "^26.1.0",
		"mdsvex": "^0.12.6",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.30.2",
		"svelte-check": "^4.2.1",
		"tailwindcss": "^4.1.7",
		"ts-node": "^10.9.2",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.32.1",
		"vite": "^6.3.5",
		"vitest": "^3.1.3"
	},
	"dependencies": {
		"@inlang/paraglide-js": "^2.0.12",
		"@libsql/client": "^0.15.6",
